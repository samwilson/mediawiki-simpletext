{
	"name": "SimpleText",
	"version": "2.0.0",
	"author": "Sam Wilson",
	"url": "https://www.mediawiki.org/wiki/Special:MyLangauge/Skin:SimpleText",
	"descriptionmsg": "simpletext-desc",
	"namemsg": "skinname-simpletext",
	"license-name": "GPL-3.0+",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.40.0"
	},
	"ValidSkinNames": {
		"simpletext": {
			"class": "SkinMustache",
			"args": [
				{
					"name": "simpletext",
					"responsive": true,
					"styles": [
						"skins.simpletext"
					],
					"messages": [
						"sitetitle"
					]
				}
			]
		}
	},
	"MessagesDirs": {
		"SimpleText": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.simpletext": {
			"features": {
				"normalize": false,
				"logo": false,
				"content-media": false,
				"content-links": false,
				"content-links-external": false,
				"content-parser-output": false,
				"content-tables": false,
				"interface": true,
				"interface-category": false,
				"interface-message-box": false,
				"elements": false,
				"legacy": false,
				"i18n-ordered-lists": false,
				"i18n-all-lists-margins": false,
				"i18n-headings": false,
				"toc": false
			},
			"styles": {
				"resources/libraries/normalize.css": {
					"media": "screen"
				},
				"resources/style.less": {
					"media": "all"
				},
				"resources/print.less": {
					"media": "print"
				}
			}
		}
	},
	"ResourceModuleSkinStyles": {
		"simpletext": {
			"+ext.echo.styles.badge": "resources/extensions/Echo.less"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "SimpleText"
	},
	"manifest_version": 2
}
